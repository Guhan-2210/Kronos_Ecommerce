{
  "all": true,
  "include": ["src/**/*.js"],
  "exclude": [
    "src/index.js",
    "src/routes/**",
    "src/migrations/**",
    "src/models/db.js",
    "src/controllers/**"
  ],
  "reporter": ["text", "text-summary", "html", "lcov"],
  "check-coverage": false,
  "lines": 70,
  "functions": 70,
  "branches": 70,
  "statements": 70
}
